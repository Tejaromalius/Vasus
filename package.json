{
  "name": "vasus",
  "displayName": "Vasus",
  "description": "Sync Asus's Keyboard Static Color to Workbench's Status Bar Color",
  "version": "1.0.4",
  "publisher": "Tejaromalius",
  "icon": "icon.png",
  "license": "MIT",
  "licenses": [
    {
      "url": "https://github.com/Tejaromalius/Vasus/blob/main/LICENSE"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Tejaromalius/Vasus.git"
  },
  "readme": "README.md",
  "activationEvents": [
    "onStartupFinished"
  ],
  "engines": {
    "vscode": "^1.87.0"
  },
  "os": [
    "linux"
  ],
  "categories": [
    "Other"
  ],
  "keywords": [
    "keyboard",
    "asus",
    "sync",
    "color",
    "status bar"
  ],
  "contributes": {
    "commands": [
      {
        "command": "vasus.syncKeyboardColor",
        "title": "Vasus: Sync Keyboard Color"
      },
      {
        "command": "vasus.toggleSyncKeyboardColor",
        "title": "Vasus: Toggle Color Sync"
      }
    ],
    "configuration": {
      "properties": {
        "vasus.syncEnabled": {
          "type": "boolean",
          "default": true,
          "description": "Enable/Disable Syncing Keyboard Color"
        }
      }
    }
  },
  "extensionDependencies": [
    "johnpapa.vscode-peacock"
  ],
  "devDependencies": {
    "@types/node": "18.x",
    "@types/vscode": "^1.87.0",
    "@vscode/vsce": "^2.24.0"
  },
  "main": "./extension.js",
  "scripts": {
    "test": "vscode-test"
  },
  "author": {
    "name": "Tejaromalius",
    "email": "tayefi.ilia@protonmail.com"
  }
}